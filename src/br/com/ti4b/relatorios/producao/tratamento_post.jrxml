	<queryString>
		<![CDATA[SELECT
     animal."cod_animal" AS animal_cod_animal,
     animal."cod_instituicao" AS animal_cod_instituicao,
     animal."cod_origem" AS animal_cod_origem,
     animal."cod_fase" AS animal_cod_fase,
     animal."nome" AS animal_nome,
     animal."sexo" AS animal_sexo,
     animal."raca" AS animal_raca,
     animal."cor" AS animal_cor,
     animal."idade" AS animal_idade,
     animal."peso" AS animal_peso,
     animal."grau_sangue" AS animal_grau_sangue,
     animal."informacao" AS animal_informacao,
     consulta."cod_consulta" AS consulta_cod_consulta,
     consulta."cod_animal" AS consulta_cod_animal,
     consulta."crmv" AS consulta_crmv,
     consulta."data_consulta" AS consulta_data_consulta,
     consulta."tipo_exame_encam" AS consulta_tipo_exame_encam,
     consulta."informacao" AS consulta_informacao,
     medicamento."cod_medicamento" AS medicamento_cod_medicamento,
     medicamento."descricao" AS medicamento_descricao,
     medicamento."eficacia" AS medicamento_eficacia,
     medicamento."quant" AS medicamento_quant,
     medicamento."preco_unit" AS medicamento_preco_unit,
     medicamento."data_validade" AS medicamento_data_validade,
     tratamento."cod_tratamento" AS tratamento_cod_tratamento,
     tratamento."cod_diagnostico" AS tratamento_cod_diagnostico,
     tratamento."cod_medicamento" AS tratamento_cod_medicamento,
     tratamento."data_tratamento" AS tratamento_data_tratamento,
     tratamento."periodo_tratamento" AS tratamento_periodo_tratamento,
     tratamento."quant" AS tratamento_quant,
     diagnostico."cod_diagnostico" AS diagnostico_cod_diagnostico,
     diagnostico."cod_consulta" AS diagnostico_cod_consulta,
     diagnostico."cod_doenca" AS diagnostico_cod_doenca,
     diagnostico."data_diagnostico" AS diagnostico_data_diagnostico,
     diagnostico."informacao" AS diagnostico_informacao
FROM
     "public"."animal" animal INNER JOIN "public"."consulta" consulta ON animal."cod_animal" = consulta."cod_animal"
     INNER JOIN "public"."diagnostico" diagnostico ON consulta."cod_consulta" = diagnostico."cod_consulta"
     INNER JOIN "public"."tratamento" tratamento ON diagnostico."cod_diagnostico" = tratamento."cod_diagnostico"
     INNER JOIN "public"."medicamento" medicamento ON tratamento."cod_medicamento" = medicamento."cod_medicamento"]]>
	</queryString>
