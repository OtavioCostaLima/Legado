	<queryString>
		<![CDATA[SELECT
     vacina_obrigatoria."cod_vacina" AS vacina_obrigatoria_cod_vacina,
     vacina_obrigatoria."descricao" AS vacina_obrigatoria_descricao,
     vacina_obrigatoria."periodo_aplicacao" AS vacina_obrigatoria_periodo_aplicacao,
     campanha."cod_campanha" AS campanha_cod_campanha,
     campanha."cod_animal" AS campanha_cod_animal,
     campanha."data_aplicacao" AS campanha_data_aplicacao,
     campanha."quant" AS campanha_quant,
     animal."nome" AS animal_nome,
     vacina_obrigatoria."preco_unit" AS vacina_obrigatoria_preco_unit
FROM
     "public"."vacina_obrigatoria" vacina_obrigatoria INNER JOIN "public"."campanha" campanha ON vacina_obrigatoria."cod_vacina" = campanha."cod_vacina"
     INNER JOIN "public"."animal" animal ON campanha."cod_animal" = animal."cod_animal"]]>
	</queryString>
