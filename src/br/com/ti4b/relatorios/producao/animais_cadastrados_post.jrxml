	<queryString>
		<![CDATA[SELECT
     animal."cod_animal" AS animal_cod_animal,
     animal."cod_instituicao" AS animal_cod_instituicao,
     animal."cod_origem" AS animal_cod_origem,
     animal."cod_fase" AS animal_cod_fase,
     animal."nome" AS animal_nome,
     animal."sexo" AS animal_sexo,
     animal."raca" AS animal_raca,
     animal."cor" AS animal_cor,
     animal."idade" AS animal_idade,
     animal."peso" AS animal_peso,
     animal."grau_sangue" AS animal_grau_sangue,
     animal."informacao" AS animal_informacao,
     fase."cod_fase" AS fase_cod_fase,
     fase."cod_clasificacao" AS fase_cod_clasificacao,
     fase."descricao" AS fase_descricao,
     fase."período" AS fase_período,
     origem."cod_origem" AS origem_cod_origem,
     origem."cod_for_animal" AS origem_cod_for_animal,
     origem."cod_leilao" AS origem_cod_leilao,
     origem."cod_nascimento" AS origem_cod_nascimento,
     origem."data_entrada" AS origem_data_entrada,
     classificacao."cod_classificacao" AS classificacao_cod_classificacao,
     classificacao."descricao" AS classificacao_descricao,
     origem_A."cod_origem" AS origem_A_cod_origem,
     origem_A."cod_for_animal" AS origem_A_cod_for_animal,
     origem_A."cod_leilao" AS origem_A_cod_leilao,
     origem_A."cod_nascimento" AS origem_A_cod_nascimento,
     origem_A."data_entrada" AS origem_A_data_entrada
FROM
     "public"."fase" fase INNER JOIN "public"."animal" animal ON fase."cod_fase" = animal."cod_fase"
     INNER JOIN "public"."origem" origem ON animal."cod_origem" = origem."cod_origem"
     INNER JOIN "public"."origem" origem_A ON animal."cod_origem" = origem_A."cod_origem"
     INNER JOIN "public"."classificacao" classificacao ON fase."cod_clasificacao" = classificacao."cod_classificacao"]]>
	</queryString>
