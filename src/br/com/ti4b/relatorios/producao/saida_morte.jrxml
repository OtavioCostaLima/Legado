	<queryString language="SQL">
		<![CDATA[SELECT
     animal."cod_animal" AS animal_cod_animal,
     animal."cod_instituicao" AS animal_cod_instituicao,
     animal."cod_origem" AS animal_cod_origem,
     animal."cod_fase" AS animal_cod_fase,
     animal."nome" AS animal_nome,
     animal."sexo" AS animal_sexo,
     animal."raca" AS animal_raca,
     animal."cor" AS animal_cor,
     animal."idade" AS animal_idade,
     animal."data_nascimento" AS animal_data_nascimento,
     animal."peso" AS animal_peso,
     animal."grau_sangue" AS animal_grau_sangue,
     animal."informacao" AS animal_informacao,
     morte."cod_morte" AS morte_cod_morte,
     morte."descricao" AS morte_descricao,
     saida."cod_saida" AS saida_cod_saida,
     saida."cod_animal" AS saida_cod_animal,
     saida."cod_leilao" AS saida_cod_leilao,
     saida."cod_morte" AS saida_cod_morte,
     saida."data_saida" AS saida_data_saida,
     saida."destino" AS saida_destino,
     saida."informacao" AS saida_informacao
FROM
     "public"."animal" animal INNER JOIN "public"."saida" saida ON animal."cod_animal" = saida."cod_animal"
     INNER JOIN "public"."morte" morte ON saida."cod_morte" = morte."cod_morte"]]>
