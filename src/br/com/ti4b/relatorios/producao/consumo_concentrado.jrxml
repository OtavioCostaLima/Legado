	<queryString>
		<![CDATA[SELECT
     consumo."cod_consumo" AS consumo_cod_consumo,
     consumo."cod_classificacao" AS consumo_cod_classificacao,
     consumo."cod_volumoso" AS consumo_cod_volumoso,
     consumo."cod_concentrado" AS consumo_cod_concentrado,
     consumo."quantidade" AS consumo_quantidade,
     consumo."data_consumo" AS consumo_data_consumo,
     concentrado."cod_concentrado" AS concentrado_cod_concentrado,
     concentrado."cod_for_alimento" AS concentrado_cod_for_alimento,
     concentrado."descricao" AS concentrado_descricao,
     concentrado."data_entrada" AS concentrado_data_entrada,
     concentrado."quant" AS concentrado_quant,
     concentrado."preco_unit" AS concentrado_preco_unit,
     classificacao."cod_classificacao" AS classificacao_cod_classificacao,
     classificacao."descricao" AS classificacao_descricao
FROM
     "public"."concentrado" concentrado INNER JOIN "public"."consumo" consumo ON concentrado."cod_concentrado" = consumo."cod_concentrado"
     INNER JOIN "public"."classificacao" classificacao ON consumo."cod_classificacao" = classificacao."cod_classificacao"]]>
