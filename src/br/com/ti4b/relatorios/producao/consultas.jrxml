	<queryString language="SQL">
		<![CDATA[SELECT
     consulta."cod_consulta" AS consulta_cod_consulta,
     consulta."cod_animal" AS consulta_cod_animal,
     consulta."crmv" AS consulta_crmv,
     consulta."data_consulta" AS consulta_data_consulta,
     consulta."tipo_exame_encam" AS consulta_tipo_exame_encam,
     consulta."informacao" AS consulta_informacao,
     animal."cod_animal" AS animal_cod_animal,
     animal."nome" AS animal_nome,
     veterinario."cod_veterinario" AS veterinario_cod_veterinario,
     veterinario."crmv" AS veterinario_crmv,
     veterinario."nome" AS veterinario_nome
FROM
     "public"."animal" animal INNER JOIN "public"."consulta" consulta ON animal."cod_animal" = consulta."cod_animal"
     INNER JOIN "public"."veterinario" veterinario ON consulta."crmv" = veterinario."crmv"]]>
	</queryString>
