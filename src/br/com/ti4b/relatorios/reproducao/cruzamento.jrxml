	<queryString>
		<![CDATA[SELECT
     cruzamento."cod_cruzamento" AS cruzamento_cod_cruzamento,
     cruzamento."cod_tipo_cruzamento" AS cruzamento_cod_tipo_cruzamento,
     cruzamento."cod_animal_mae" AS cruzamento_cod_animal_mae,
     cruzamento."crmv" AS cruzamento_crmv,
     cruzamento."data_cruza" AS cruzamento_data_cruza,
     cruzamento."pai" AS cruzamento_pai,
     cruzamento."tentativa" AS cruzamento_tentativa,
     cruzamento."status" AS cruzamento_status,
     animal."cod_animal" AS animal_cod_animal,
     animal."cod_instituicao" AS animal_cod_instituicao,
     animal."cod_origem" AS animal_cod_origem,
     animal."cod_fase" AS animal_cod_fase,
     animal."nome" AS animal_nome,
     animal."sexo" AS animal_sexo,
     animal."raca" AS animal_raca,
     animal."cor" AS animal_cor,
     animal."idade" AS animal_idade,
     animal."data_nascimento" AS animal_data_nascimento,
     animal."peso" AS animal_peso,
     animal."grau_sangue" AS animal_grau_sangue,
     animal."informacao" AS animal_informacao,
     veterinario."cod_veterinario" AS veterinario_cod_veterinario,
     veterinario."crmv" AS veterinario_crmv,
     veterinario."cpf" AS veterinario_cpf,
     veterinario."nome" AS veterinario_nome,
     veterinario."endereco" AS veterinario_endereco,
     veterinario."numero" AS veterinario_numero,
     veterinario."bairro" AS veterinario_bairro,
     veterinario."cidade" AS veterinario_cidade,
     veterinario."uf" AS veterinario_uf,
     veterinario."email" AS veterinario_email,
     veterinario."fone1" AS veterinario_fone1,
     veterinario."fone2" AS veterinario_fone2,
     veterinario."tipo_contrato" AS veterinario_tipo_contrato,
     veterinario."especialidade" AS veterinario_especialidade,
     tipo_cruzamento."cod_tipo_cruzamento" AS tipo_cruzamento_cod_tipo_cruzamento,
     tipo_cruzamento."descricao" AS tipo_cruzamento_descricao,
     tipo_cruzamento_A."cod_tipo_cruzamento" AS tipo_cruzamento_A_cod_tipo_cruzamento,
     tipo_cruzamento_A."descricao" AS tipo_cruzamento_A_descricao
FROM
     "public"."animal" animal INNER JOIN "public"."cruzamento" cruzamento ON animal."cod_animal" = cruzamento."cod_animal_mae"
     INNER JOIN "public"."veterinario" veterinario ON cruzamento."crmv" = veterinario."crmv"
     INNER JOIN "public"."tipo_cruzamento" tipo_cruzamento ON cruzamento."cod_tipo_cruzamento" = tipo_cruzamento."cod_tipo_cruzamento"
     INNER JOIN "public"."tipo_cruzamento" tipo_cruzamento_A ON cruzamento."cod_tipo_cruzamento" = tipo_cruzamento_A."cod_tipo_cruzamento"]]>
	</queryString>
